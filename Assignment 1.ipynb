        " # Function to create numpy linspace\n",
        "def linspacefunction(l, h, n) :\n",
        "    hl = h - l\n",
        "    diff = hl / (n - 1)\n",
        "    ans = []\n",
        "    for i in range(n) :\n",
        "        ans.append(l)\n",
        "        l = l + diff\n",
        "    return ans\n",
        "\n",
        "linspacefunction(10,30,10)"
      ],
